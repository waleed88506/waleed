#include <iostream>
using namespace std;




int main()
{
	int max, secondnumber, x, i;

	cout << " Enter 10 number :" << endl;

	cin >> x;
	max = x;
	secondnumber = x;
	
	for (i = 1; i <= 9; i++)
	{
		cin >> x;
		if (x > max)
		{

			secondnumber = max;
			max = x;
		}
	}

	cout <<"the max number  "<< max << endl;
	cout << " the scecondnumber " << secondnumber << endl;


	system("pause");
	return 0;
}
