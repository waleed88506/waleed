#include <iostream>
using namespace std;


template < typename t >
class tclas
{
	t v1, v2;
public:
	tclas(t f, t s)
	{
		v1 = f; v2 = s;
	}

	t getmax();

};

template < class t >
t tclas<t>::getmax()
{
	t r;
	r = v1 > v2 ? v1 : v2;

	return r;
}



int main()
{
	tclas <int> myobject (100, 87);
	cout << myobject.getmax();



	system("pause");
	return 0;
}
